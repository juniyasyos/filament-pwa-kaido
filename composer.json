{
    "name": "juniyasyos/filament-pwa-kaido",
    "type": "library",
    "description": "get a PWA feature on your FilamentPHP app with settings from panel",
    "keywords": [
        "php",
        "laravel",
        "pwa",
        "filament",
        "tomatophp",
        "juniyasyos",
        "filament-plugin"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Juniyasyos\\FilamentPWA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juniyasyos\\FilamentPWA\\FilamentPwaServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Ahmad Ilyas",
            "email": "ahmadilyasdahlan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "juniyasyos/filament-settings-hub-kaido": "^3.0",
        "tomatophp/console-helpers": "^1.1"
    }
}
